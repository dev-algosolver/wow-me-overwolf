{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "WoW.me",
    "author": "Ohad",
    "version": "1.0.2",
    "minimum-overwolf-version": "0.169.0",
    "description": "Your guide to be a better WoW player. Talent picker, track activities and gear recommendations",
    "dock_button_title": "WoW.me",
    "icon": "icons/IconMouseOver.png",
    "icon_gray": "icons/IconMouseNormal.png",
    "launcher_icon": "icons/desktopIcon.ico",
    "window_icon": "icons/TaskbarIcon.png"
  },
  "permissions": [
    "Hotkeys",
    "GameInfo",
    "Extensions",
    "Streaming",
    "Profile"
  ],
  "data": {
    "start_window": "background",
    "windows": {
      "background": {
        "file": "background.html",
        "background_optimization": false,
        "is_background_page": true
      },
      "desktop": {
        "file": "in_game.html",
        "desktop_only": true,
        "native_window": true,
        "resizable": true,
        "transparent": true,
        "override_on_update": true,
        "size": {
          "width": 1212,
          "height": 699
        },
        "min_size": {
          "width": 1212,
          "height": 699
        },
        "max_size": {
          "width": 1212,
          "height": 699
        },
        "block_top_window_navigation": true,
        "popup_blocker": true,
        "mute": true
      },
      "in_game": {
        "file": "in_game.html",
        "in_game_only": true,
        "focus_game_takeover": "ReleaseOnHidden",
        "focus_game_takeover_release_hotkey": "toggle_visibility",
        "transparent": true,
        "override_on_update": true,
        "size": {
          "width": 805,
          "height": 780
        },
        "min_size": {
          "width": 805,
          "height": 670
        },
        "max_size": {
          "width": 805,
          "height": 780
        },
        "block_top_window_navigation": true,
        "popup_blocker": true,
        "mute": true
      }
    },
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [
        765
      ]
    },
    "game_events": [
      765
    ],
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [
            765
          ]
        }
      }
    ],
    "hotkeys": {
      "showhide": {
        "title": "Show/Hide In-Game Window",
        "action-type": "toggle",
        "default": "Ctrl+F8"
      }
    },
    "protocol_override_domains": {
      "googlesyndication": "http"
    },
    "externally_connectable": {
      "matches": [
        "http://*.overwolf.com",
        "https://*.overwolf.com",
        "https://overwolf.github.io",
        "https://*.google-analytics.com",
        "http://*.google-analytics.com",
        "https://www.googleapis.com",
        "https://discord.gg/v5cfBTq",
        "https://twitter.com/",
        "https://www.facebook.com",
        "https://www.reddit.com",
        "https://wowme.gg",
        "https://wowme.gg/oauth/callback_overwolf"
      ]
    },
    "url_protocol": {
      "scheme": "wowme"
    },
    "force_browser": "user",
    "developer": {
      "enable_auto_refresh": true,
      "reload_delay": 1000,
      "filter": "*.*"
    }
  }
}